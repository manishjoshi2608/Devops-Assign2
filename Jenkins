pipeline {
    agent any

    stages {
        stage('Build ') {
            steps {
                echo 'Building is  in progress......'
            }
        }
    
    
        stage('test') {
            steps {
                echo 'Testing .....'
            }
        }
    
    
        stage('deploy') {
            steps {
                bat 'node hello'
                echo 'Deploying....... '
            }
        }
    }
    
}
