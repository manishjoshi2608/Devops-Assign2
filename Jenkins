pipeline {
    agent any

    stages {
        stage('Build ') {
            steps {
                echo 'Hello This is Build'
            }
        }
    
    
        stage('test') {
            steps {
                echo 'Hello this is test'
            }
        }
    
    
        stage('deploy') {
            steps {
                node hello
                echo 'Hello this is deploy '
            }
        }
    }
    
}
